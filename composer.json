{
  "name": "sowailem/ownable",
  "description": "A Laravel package to handle ownership relationships between models",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Abdullah Sowailem",
      "email": "abdullah.sowailem@email.com"
    }
  ],
  "require": {
    "php": "^8.3",
    "illuminate/contracts": "^12.0",
    "illuminate/support": "^12.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0",
    "phpstan/phpstan": "^2.0",
    "squizlabs/php_codesniffer": "^3.10",
    "orchestra/testbench": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Sowailem\\Ownable\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sowailem\\Ownable\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Sowailem\\Ownable\\OwnableServiceProvider"
      ],
      "aliases": {
        "Owner": "Sowailem\\Ownable\\Facades\\Owner"
      }
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
